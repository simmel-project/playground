{
    "configurations": [
        {
            "name": "Simmel",
            "defines": [
                "ARM_MATH_CM4",
                "__FPU_PRESENT=1",
                "NRF52833_XXAA",
                "S140",
                "CFG_TUSB_MCU=OPT_MCU_NRF5X"
            ],
            "compilerPath": "C:\\ProgramData\\chocolatey\\bin\\arm-none-eabi-gcc.exe",
            "cStandard": "gnu11",
            "cppStandard": "c++17",
            "includePath": [
                "${default}",
                "${workspaceFolder}/src/cmsis/include",
                "${workspaceFolder}/lib/nrfx/hal",
                "${workspaceFolder}/lib/nrfx/drivers/include",
                "${workspaceFolder}/lib/nrfx",
                "${workspaceFolder}/lib/nrfx/templates/nRF52833",
                "${workspaceFolder}/lib/nrfx/mdk",
                "${workspaceFolder}/lib/nrfx/templates",
                "${workspaceFolder}/lib/tinyusb/src"
            ],
            "compilerArgs": [
                "-mthumb",
                "-mabi=aapcs",
                "-mcpu=cortex-m4",
                "-mtune=cortex-m4",
                "-mfloat-abi=hard",
                "-mfpu=fpv4-sp-d16",
                "-ffunction-sections",
                "-fdata-sections",
                "-fno-builtin",
                "-fshort-enums",
                "-fstack-usage",
                "-fno-strict-aliasing"
            ],
            "intelliSenseMode": "gcc-arm"
        }
    ],
    "version": 4
}
